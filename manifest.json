{
  "manifest_version": 3,
  "name": "网页使用时长",
  "version": "1.0",
  "description": "记录网页使用时长及打开次数",
  "permissions": ["tabs", "activeTab", "storage", "fileSystem"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  }
}
